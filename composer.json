{
  "name": "znck/trust",
  "type": "library",
  "description": "User roles and permissions for Laravel 5",
  "homepage": "https://github.com/znck/trust",
  "keywords": ["laravel", "role", "roles", "permissions", "trust", "authorisation", "gate"],
  "require": {
    "php": ">=7.0",
    "illuminate/database": "^5.2",
    "illuminate/contracts": "^5.2",
    "illuminate/support": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "graham-campbell/testbench": "^3.1",
    "fabpot/php-cs-fixer": "^1.11",
    "phpunit/php-code-coverage": "^3.3",
    "satooshi/php-coveralls": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Rahul Kadyan",
      "email": "hi@znck.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "Znck\\Trust\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Znck\\Tests\\Trust\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit",
    "cs": "php-cs-fixer fix"
  }
}
